<%= simple_form_for @project do |f| %>
    <%= f.input :name %>
    <%= f.button :submit, class: "btn primary" %>
    <% if @project.id %>
        <%= link_to 'cancel', project_path(@project), class: "btn" %>
    <% else %>
        <%= link_to 'cancel', projects_path, class: "btn" %>
    <% end %>
<% end %>
